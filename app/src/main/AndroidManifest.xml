<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.Ashish.foodnetwork">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodNetwork"
        tools:replace="android:theme">
        <activity
            android:name=".UserDashboard.TermsandConditionActivity"
            android:exported="false" />
        <activity
            android:name=".admin.addCategory.ListCategoryActivity"
            android:exported="false" />
        <activity
            android:name=".UserDashboard.AboutUsActivity"
            android:exported="false" />
        <activity
            android:name=".admin.products.ListProductsActivity"
            android:exported="false" />
        <activity
            android:name=".admin.addProduct.AddProductActivity"
            android:exported="false" />
        <activity
            android:name=".UserDashboard.AddAddressActivity"
            android:exported="false" />
        <activity
            android:name=".UserDashboard.OrderHistoryDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".UserDashboard.OrderCompleteActivity"
            android:exported="false" />
        <activity
            android:name=".UserDashboard.AddressActivity"
            android:exported="false" />
        <activity
            android:name=".admin.AdminActivity"
            android:exported="false" />
        <activity
            android:name=".UserDashboard.CheckOutActivity"
            android:exported="false" />
        <activity
            android:name=".UserDashboard.FoodDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".UserDashboard.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".userAccount.UserAccountActivity"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>